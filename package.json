  "private": true,
  "scripts": {
    "lint": "eslint *.js src tools",
    "relay-compiler": "relay-compiler --src src --exclude 'data/**' --schema data/schema.graphql",
    "build": "npm run relay-compiler && babel src --out-dir lib --copy-files && webpack --config webpack.config.js",
    "prestart": "npm run update-schema && npm run build",
    "start": "node lib/server",
    "start-test-env": "node lib/server",
    "start-relay-dev": "relay-compiler --watch --verbose --src src --exclude 'data/**' --schema data/schema.graphql",
    "build-dev": "npm run update-schema && babel --watch src --out-dir lib --copy-files",
    "webpack-dev": "webpack --progress --colors --watch --config webpack.dev.js",
    "start-dev": "nodemon ./lib/server.js",
    "update-schema": "babel-node tools/updateSchema.js"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "npm run update-schema",
      "git add src/data/schema.*"
    ]
  },
  "prettier": {
    "printWidth": 79,
    "singleQuote": true,
    "trailingComma": "all"
